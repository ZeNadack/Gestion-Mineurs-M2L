<Window x:Class="CentreLoisirs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CentreLoisirs"
        mc:Ignorable="d"
        Title="Gestion adherents Mineurs de la M2L" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Accueil" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="24" FontFamily="Segoe Print"/>
        <Button x:Name="BtnFamilles" Content="Familles" HorizontalAlignment="Left" Margin="158,135,0,0" VerticalAlignment="Top" Height="39" Width="100" Click="BtnFamilles_Click"/>
        <Button x:Name="BtnMedecins" Content="Médecins" HorizontalAlignment="Left" Margin="551,135,0,0" VerticalAlignment="Top" Height="39" Width="100" Click="BtnMedecins_Click"/>
        <Button x:Name="BtnEnfants" Content="Enfants" HorizontalAlignment="Center" Margin="0,233,0,0" VerticalAlignment="Top" Height="39" Width="100" Click="BtnEnfants_Click"/>

    </Grid>
</Window>
